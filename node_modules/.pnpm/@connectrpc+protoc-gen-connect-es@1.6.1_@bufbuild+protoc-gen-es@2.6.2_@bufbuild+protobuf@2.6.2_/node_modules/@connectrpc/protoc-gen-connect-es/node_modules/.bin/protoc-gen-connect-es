#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/@connectrpc+protoc-gen-connect-es@1.6.1_@bufbuild+protoc-gen-es@2.6.2_@bufbuild+protobuf@2.6.2_/node_modules/@connectrpc/protoc-gen-connect-es/bin/node_modules:/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/@connectrpc+protoc-gen-connect-es@1.6.1_@bufbuild+protoc-gen-es@2.6.2_@bufbuild+protobuf@2.6.2_/node_modules/@connectrpc/protoc-gen-connect-es/node_modules:/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/@connectrpc+protoc-gen-connect-es@1.6.1_@bufbuild+protoc-gen-es@2.6.2_@bufbuild+protobuf@2.6.2_/node_modules/@connectrpc/node_modules:/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/@connectrpc+protoc-gen-connect-es@1.6.1_@bufbuild+protoc-gen-es@2.6.2_@bufbuild+protobuf@2.6.2_/node_modules:/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/@connectrpc+protoc-gen-connect-es@1.6.1_@bufbuild+protoc-gen-es@2.6.2_@bufbuild+protobuf@2.6.2_/node_modules/@connectrpc/protoc-gen-connect-es/bin/node_modules:/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/@connectrpc+protoc-gen-connect-es@1.6.1_@bufbuild+protoc-gen-es@2.6.2_@bufbuild+protobuf@2.6.2_/node_modules/@connectrpc/protoc-gen-connect-es/node_modules:/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/@connectrpc+protoc-gen-connect-es@1.6.1_@bufbuild+protoc-gen-es@2.6.2_@bufbuild+protobuf@2.6.2_/node_modules/@connectrpc/node_modules:/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/@connectrpc+protoc-gen-connect-es@1.6.1_@bufbuild+protoc-gen-es@2.6.2_@bufbuild+protobuf@2.6.2_/node_modules:/Users/ethantlee/Sandbox/pi/_tmp_pi_protos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/protoc-gen-connect-es" "$@"
else
  exec node  "$basedir/../../bin/protoc-gen-connect-es" "$@"
fi
